python
â€Žimport streamlit as st
â€Žimport random
â€Ž
â€Ždef get_color(num):
â€Ž    if num in [1, 3, 7, 9]:
â€Ž        return "Red"
â€Ž    elif num in [0, 2, 4, 6, 8]:
â€Ž        return "Green"
â€Ž    else:
â€Ž        return "Violet"
â€Ž
â€Žst.title("ðŸŽ¯ Wingo AI Color Predictor")
â€Žst.write("Last 3 Wingo numbers dijiye:")
â€Ž
â€Žn1 = st.number_input("Pehla number", 0, 9, step=1)
â€Žn2 = st.number_input("Doosra number", 0, 9, step=1)
â€Žn3 = st.number_input("Teesra number", 0, 9, step=1)
â€Ž
â€Žif st.button("Predict"):
â€Ž    nums = [n1, n2, n3]
â€Ž    colors = [get_color(int(n)) for n in nums]
â€Ž    result = random.choice(["Red", "Green", "Violet"])
â€Ž    st.success(f"Prediction: {result} (based on {colors})")
â€Ž
